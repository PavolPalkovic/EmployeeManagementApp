{"ast":null,"code":"import axios from 'axios';\nexport default {\n  props: ['id', 'firstName', 'lastName', 'positionName'],\n  components: {},\n\n  data() {\n    return {\n      employeeData: {\n        firstName: this.firstName,\n        lastName: this.lastName,\n        positionName: this.positionName\n      },\n      positionData: [],\n      currentDate: new Date().toJSON().slice(0, 10).replace(/-/g, '/')\n    };\n  },\n\n  mounted() {\n    this.getPositionData();\n    this.currentDateFormating();\n  },\n\n  methods: {\n    // Custom event. Parent component listens to event named \"closeEdit\".\n    // $emit('nameOfEvent')\n    closeEdit() {\n      this.$emit('closeEdit');\n    },\n\n    editEmployee() {\n      axios.put('http://localhost:1028/api/employees/' + this.id, this.employeeData).then(response => console.log(response)).catch(error => console.log(error));\n    },\n\n    getPositionData() {\n      fetch('http://localhost:1028/api/positions').then(response => response.json()).then(data => {\n        this.positionData = data;\n      });\n    },\n\n    reloadPage() {\n      window.location.reload();\n    },\n\n    currentDateFormating() {\n      currentDate = new Date().toJSON().slice(0, 10).replace(/-/g, '/');\n      console.log(currentDate);\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAqBA,OAAOA,KAAP,MAAkB,OAAlB;AAEA,eAAe;AACXC,OAAK,EAAE,CAAE,IAAF,EAAQ,WAAR,EAAqB,UAArB,EAAiC,cAAjC,CADI;AAEXC,YAAU,EAAE,EAFD;;AAGXC,MAAI,GAAG;AACH,WAAO;AACHC,kBAAY,EAAE;AACVC,iBAAS,EAAE,KAAKA,SADN;AAEVC,gBAAQ,EAAE,KAAKA,QAFL;AAGVC,oBAAY,EAAE,KAAKA;AAHT,OADX;AAMHC,kBAAY,EAAE,EANX;AAOHC,iBAAW,EAAE,IAAIC,IAAJ,GAAWC,MAAX,GAAoBC,KAApB,CAA0B,CAA1B,EAA4B,EAA5B,EAAgCC,OAAhC,CAAwC,IAAxC,EAA6C,GAA7C;AAPV,KAAP;AASH,GAbU;;AAcXC,SAAO,GAAG;AACN,SAAKC,eAAL;AACA,SAAKC,oBAAL;AACH,GAjBU;;AAkBXC,SAAO,EAAE;AACL;AACA;AACAC,aAAS,GAAG;AACR,WAAKC,KAAL,CAAW,WAAX;AACH,KALI;;AAMLC,gBAAY,GAAG;AACXpB,WAAK,CAACqB,GAAN,CAAU,yCAAyC,KAAKC,EAAxD,EAA4D,KAAKlB,YAAjE,EACKmB,IADL,CACUC,QAAO,IAAKC,OAAO,CAACC,