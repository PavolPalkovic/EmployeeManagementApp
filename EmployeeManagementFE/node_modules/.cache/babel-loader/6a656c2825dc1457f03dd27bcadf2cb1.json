{"ast":null,"code":"import axios from 'axios';\nimport DatePicker from 'vue2-datepicker';\nimport 'vue2-datepicker/index.css';\nexport default {\n  props: ['id', 'firstName', 'lastName', 'positionName'],\n  components: {\n    DatePicker\n  },\n\n  data() {\n    return {\n      employeeData: {\n        firstName: this.firstName,\n        lastName: this.lastName,\n        positionName: this.positionName\n      },\n      time1: null,\n      positionData: []\n    };\n  },\n\n  mounted() {\n    this.getPositionData();\n  },\n\n  methods: {\n    // Custom event. Parent component listens to event named \"closeEdit\".\n    // $emit('nameOfEvent')\n    closeEdit() {\n      this.$emit('closeEdit');\n    },\n\n    editEmployee() {\n      axios.put('http://localhost:1028/api/employees/' + this.id, this.employeeData).then(response => console.log(response)).catch(error => console.log(error));\n    },\n\n    getPositionData() {\n      fetch('http://localhost:1028/api/positions').then(response => response.json()).then(data => {\n        this.positionData = data;\n      });\n    },\n\n    reloadPage() {\n      window.location.reload();\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAqBA,OAAOA,KAAP,MAAkB,OAAlB;AACA,OAAOC,UAAP,MAAuB,iBAAvB;AACA,OAAO,2BAAP;AAEA,eAAe;AACXC,OAAK,EAAE,CAAE,IAAF,EAAQ,WAAR,EAAqB,UAArB,EAAiC,cAAjC,CADI;AAEXC,YAAU,EAAE;AAAEF;AAAF,GAFD;;AAGXG,MAAI,GAAG;AACH,WAAO;AACHC,kBAAY,EAAE;AACVC,iBAAS,EAAE,KAAKA,SADN;AAEVC,gBAAQ,EAAE,KAAKA,QAFL;AAGVC,oBAAY,EAAE,KAAKA;AAHT,OADX;AAMHC,WAAK,EAAE,IANJ;AAOHC,kBAAY,EAAE;AAPX,KAAP;AASH,GAbU;;AAcXC,SAAO,GAAG;AACN,SAAKC,eAAL;AACH,GAhBU;;AAiBXC,SAAO,EAAE;AACL;AACA;AACAC,aAAS,GAAG;AACR,WAAKC,KAAL,CAAW,WAAX;AACH,KALI;;AAMLC,gBAAY,GAAG;AACXhB,WAAK,CAACiB,GAAN,CAAU,yCAAyC,KAAKC,EAAxD,EAA4D,KAAKb,YAAjE,EACKc,IADL,CACUC,QAAO,IAAKC,OAAO,CAACC,GAAR,CAAYF,QAAZ,CADtB,EAEKG,KAFL,CAEWC,KAAI,IAAKH,OAAO,CAACC,GAAR,CAAYE,KAAZ,CAFpB;AAGH,KAVI;;AAWLZ,mBAAe,GAAG;AACda,WAAK,CAAC,qCAAD,CAAL,CACKN,IADL,CACUC,QAAO,IAAKA,QAAQ,CAACM,IAAT,EADtB,EAEKP,