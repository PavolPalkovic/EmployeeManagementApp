{"ast":null,"code":"import HistoryDelete from '../components/HistoryView/HistoryDelete.vue';\nimport HistoryDetails from '../components/HistoryView/HistoryDetails.vue';\nexport default {\n  name: 'HistoryView',\n  components: {\n    HistoryDelete\n  },\n\n  data() {\n    return {\n      employees: [],\n      employeeHistoryId: 0,\n      employeeFirstName: \"\",\n      employeeLastName: \"\",\n      componentName: \"\"\n    };\n  },\n\n  mounted() {\n    fetch('http://localhost:1028/api/employeesHistory').then(response => response.json()).then(data => {\n      this.employees = data;\n    });\n  },\n\n  methods: {\n    passValues(emp) {\n      this.employeeHistoryId = emp.id;\n      this.employeeFirstName = emp.firstName;\n      this.employeeLastName = emp.lastName;\n      this.employeeAdress = emp.address;\n      this.employeeDateOfBirth = emp.dateOfBirth;\n      this.employeePosition = emp.positionName;\n      this.employeeSalary = emp.salary;\n    },\n\n    showComponent(emp, componentName) {\n      this.passValues(emp);\n      this.componentName = componentName;\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAyCA,OAAOA,aAAP,MAA0B,6CAA1B;AACA,OAAOC,cAAP,MAA2B,8CAA3B;AAEA,eAAe;AACbC,MAAI,EAAE,aADO;AAEbC,YAAU,EAAE;AAAEH;AAAF,GAFC;;AAIbI,MAAI,GAAG;AACL,WAAO;AACLC,eAAS,EAAE,EADN;AAELC,uBAAiB,EAAE,CAFd;AAGLC,uBAAiB,EAAE,EAHd;AAILC,sBAAgB,EAAE,EAJb;AAKLC,mBAAa,EAAE;AALV,KAAP;AAOD,GAZY;;AAabC,SAAO,GAAG;AACRC,SAAK,CAAC,4CAAD,CAAL,CACGC,IADH,CACQC,QAAO,IAAKA,QAAQ,CAACC,IAAT,EADpB,EAEGF,IAFH,CAEQR,IAAG,IAAK;AAAC,WAAKC,SAAL,GAAiBD,IAAjB;AAAsB,KAFvC;AAGD,GAjBY;;AAkBbW,SAAO,EAAE;AACPC,cAAU,CAACC,GAAD,EAAM;AACd,WAAKX,iBAAL,GAAyBW,GAAG,CAACC,EAA7B;AACA,WAAKX,iBAAL,GAAyBU,GAAG,CAACE,SAA7B;AACA,WAAKX,gBAAL,GAAwBS,GAAG,CAACG,QAA5B;AACA,WAAKC,cAAL,GAAsBJ,GAAG,CAACK,OAA1B;AACA,WAAKC,mBAAL,GAA2BN,GAAG,CAACO,WAA/B;AACA,WAAKC,gBAAL,GAAwBR,GAAG,CAACS,YAA5B;AACA,WAAKC,cAAL,GAAsBV,GAAG,CAACW,MAA1B;AACD,KATM;;AAUPC,iBAAa,CAACZ,GAAD,EAAMR,aAAN,EAAqB;AAChC,WAAKO,UAAL,CAAgBC,GAAhB;AACA,WAAK